{
  "name": "@primno/core",
  "repository": "github:primno/core",
  "author": "Xavier Monin",
  "description": "Primno. Framework Typescript pour Dynamics 365.",
  "version": "0.2.0",
  "main": "dist/primno.cjs.js",
  "module": "dist/primno.esm.js",
  "types": "dist/primno.d.ts",
  "dist": {
    "d365-esm": "dist/primno-d365.esm.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.18.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.11.6",
    "@types/sinon": "^10.0.6",
    "@types/xrm": "^9.0.41",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rollup": "^2.59.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^10.0.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "xrm-mock": "^3.5.7",
    "reflect-metadata": "^0.1.13"
  },
  "engines": {
    "node": ">=14.5.0"
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint . --ext .ts",
    "bundle:dev": "npm run build & rollup -c",
    "bundle:prod": "npm run build & rollup -c --environment NODE_ENV:production",
    "watch": "rollup -c -w",
    "test": "nyc mocha -a -r ts-node/register test/**/*.spec.ts"
  },
  "keywords": [
    "primno",
    "dynamics 365",
    "dataverse",
    "framework",
    "feature"
  ],
  "license": "MIT",
  "browserslist": [
    "> 5%"
  ],
  "eslintIgnore": [
    "build",
    "dist",
    "node_modules"
  ]
}
